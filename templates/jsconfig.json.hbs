{
  "compilerOptions": {
    "moduleResolution": "Node",
    "module": "esnext",
    "target": "esnext",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@nodes/": ["./src/nodes"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*"],
  "typeAcquisition": {
    "enable": true,
    "include": ["jquery"]
  }
}
